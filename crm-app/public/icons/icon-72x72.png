<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8b5cf6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ec4899;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8fafc;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" rx="80" ry="80" fill="url(#bgGradient)"/>
  
  <!-- Shadow effect -->
  <rect x="20" y="20" width="472" height="472" rx="60" ry="60" fill="rgba(0,0,0,0.1)"/>
  
  <!-- Main icon background -->
  <rect x="10" y="10" width="492" height="492" rx="70" ry="70" fill="url(#bgGradient)"/>
  
  <!-- Building icon -->
  <g transform="translate(128, 100)">
    <!-- Main building -->
    <rect x="50" y="80" width="156" height="280" fill="url(#iconGradient)" rx="8"/>
    
    <!-- Building details -->
    <rect x="70" y="100" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    <rect x="111" y="100" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    <rect x="152" y="100" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    
    <rect x="70" y="140" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    <rect x="111" y="140" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    <rect x="152" y="140" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    
    <rect x="70" y="180" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    <rect x="111" y="180" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    <rect x="152" y="180" width="25" height="25" fill="rgba(139,92,246,0.3)" rx="3"/>
    
    <!-- Door -->
    <rect x="111" y="280" width="35" height="80" fill="rgba(139,92,246,0.5)" rx="5"/>
    <circle cx="135" cy="320" r="3" fill="rgba(139,92,246,0.8)"/>
    
    <!-- Side building -->
    <rect x="0" y="140" width="60" height="220" fill="rgba(255,255,255,0.9)" rx="6"/>
    <rect x="10" y="160" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
    <rect x="35" y="160" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
    <rect x="10" y="190" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
    <rect x="35" y="190" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
    
    <!-- Right building -->
    <rect x="196" y="120" width="60" height="240" fill="rgba(255,255,255,0.9)" rx="6"/>
    <rect x="206" y="140" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
    <rect x="231" y="140" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
    <rect x="206" y="170" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
    <rect x="231" y="170" width="15" height="15" fill="rgba(139,92,246,0.2)" rx="2"/>
  </g>
  
  <!-- Tailoring elements -->
  <g transform="translate(140, 400)">
    <!-- Needle -->
    <line x1="0" y1="0" x2="40" y2="-40" stroke="url(#iconGradient)" stroke-width="3" stroke-linecap="round"/>
    <circle cx="0" cy="0" r="4" fill="url(#iconGradient)"/>
    
    <!-- Thread -->
    <path d="M 0 0 Q 20 10 40 -40" stroke="rgba(255,255,255,0.6)" stroke-width="2" fill="none"/>
    
    <!-- Scissors -->
    <g transform="translate(180, 0)">
      <ellipse cx="0" cy="0" rx="8" ry="15" fill="url(#iconGradient)" transform="rotate(-20)"/>
      <ellipse cx="15" cy="0" rx="8" ry="15" fill="url(#iconGradient)" transform="rotate(20)"/>
      <line x1="-5" y1="10" x2="20" y2="10" stroke="rgba(139,92,246,0.8)" stroke-width="2"/>
    </g>
  </g>
  
  <!-- Accent elements -->
  <circle cx="100" cy="150" r="8" fill="rgba(255,255,255,0.3)"/>
  <circle cx="420" cy="200" r="12" fill="rgba(255,255,255,0.2)"/>
  <circle cx="450" cy="350" r="6" fill="rgba(255,255,255,0.4)"/>
</svg>