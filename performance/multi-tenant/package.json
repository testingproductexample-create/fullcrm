{
  "name": "@uae-tailoring/multi-tenant",
  "version": "1.0.0",
  "description": "Comprehensive multi-tenant architecture and data isolation system for UAE tailoring business platform",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "compile": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "multi-tenant",
    "saas",
    "tenant-isolation",
    "role-based-access-control",
    "rbac",
    "white-label",
    "uae",
    "tailoring",
    "business-platform",
    "typescript",
    "express",
    "postgresql",
    "redis",
    "data-isolation",
    "security",
    "onboarding"
  ],
  "author": {
    "name": "UAE Tailoring Platform Team",
    "email": "dev@uae-tailoring.com",
    "url": "https://github.com/uae-tailoring"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uae-tailoring/multi-tenant.git"
  },
  "bugs": {
    "url": "https://github.com/uae-tailoring/multi-tenant/issues"
  },
  "homepage": "https://github.com/uae-tailoring/multi-tenant#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "pg": "^8.10.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.js"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.js"
    },
    "./guards": {
      "import": "./dist/guards/index.js",
      "require": "./dist/guards/index.js"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    }
  }
}
