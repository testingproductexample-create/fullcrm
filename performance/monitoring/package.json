{
  "name": "performance-monitoring-system",
  "version": "1.0.0",
  "description": "Comprehensive real-time performance monitoring and analytics system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "db:migrate": "node utilities/database-connection.js migrate",
